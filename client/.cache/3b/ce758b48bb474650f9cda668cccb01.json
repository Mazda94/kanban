      endpoint_project: \"https://api-kanban.herokuapp.com/project\"\n    };\n  },\n  methods: {\n    signOut: function() {\n      var auth2 = gapi.auth2.getAuthInstance();\n      auth2.signOut().then(function() {\n        console.log(\"User signed out.\");\n      });\n    },\n    test: function(value) {\n      this.$emit(\"load-category\", value);\n    },\n    logout: function() {\n      this.signOut();\n      localStorage.removeItem(\"token\");\n      localStorage.removeItem(\"email\");\n      this.$emit(\"logout\");\n    },\n    request: function(params) {\n      const { method, data } = params;\n      return axios({\n        method: method,\n        url: this.endpoint_project,\n        data,\n        headers: {\n          token: localStorage.token\n        }\n      });\n    },\n    close: function() {\n      this.showForm = !this.showForm;\n      this.$emit(\"close\");\n    },\n    getProject: function() {\n      this.request({ method: \"get\" })\n        .then(res => {\n          this.projects = res.data;\n        })\n        .catch(err => {\n          this.alert({\n            title: \"Failed\",\n            icon: \"warning\",\n            msg: err\n          });\n        });\n    }\n  },\n  created: function() {\n    this.getProject();\n  },\n  alert: function({ title, icon, msg }) {\n    return Swal.fire({\n      title: title,\n      icon: icon,\n      text: msg\n    });\n  }\n};\n</script>\n\n<style>\n</style>"},"lineCount":null}},"error":null,"hash":"7e9195e83fe8cc296850cf26b2783004","cacheData":{"env":{}}}